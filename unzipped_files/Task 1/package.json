{
  "name": "vite-vanilla-js-template",
  "description": "ViteJS starter template for vanilla JavaScript.",
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "vanilla",
    "javascript",
    "template",
    "boilerplate",
    "boilerplate-template",
    "postcss",
    "prettier",
    "scaffold",
    "scaffold-template",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template"
  ],
  "scripts": {
    "dev": "vite --host --open",
    "format": "prettier --write \"./**/*.{js,jsx,json,css}\"",
    "build": "vite build",
    "preview": "vite preview --open",
    "buildpreview": "vite build && vite preview --open"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "prettier": "^3.2.5",
    "vite": "^6.2.5"
  },
  "dependencies": {
    "the-new-css-reset": "^1.11.2"
  }
}
